
.wrap{
    display: flex;
    background: #f8f8f8;
    width: 100%;
    height: 100%;
}
.navLeft{
    width: 20%;
    /*border-right: 1px solid #ccc; */
    background: #ccc;
    height: 100%;
}
.navLeft text{
    display: block;
    font-size: 14px;
    text-align: center;
    line-height: 98rpx;
    height: 98rpx;
    border-bottom: 1px solid #ccc; 
}
.navLeft .active{
    box-sizing: border-box;
    background: #fff;
}
.navRight{
    width:80%;
    height: 100%;
}
.navRight-main-item{
    width: 100%;
    height: 100%;
    display: flex;
}
.navRight-main-item-list{
    width: 100%;
    display: -webkit-flex;
    flex-wrap:wrap;
}

.navRight-main-item-list navigator{
    width: 33.33%;
    text-align: center;
}
.navRight-main-item-list navigator image{
    width:60px;
    height: 60px;
}
.navRight-main-item-list navigator text{
    display: block;
    width: 100%;
    font-size: 12px;
    color: #666;
    margin: 12px 0;  
}